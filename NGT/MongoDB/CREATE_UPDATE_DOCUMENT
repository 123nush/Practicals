...         "username":"Strokes",
...         "name":"Ben Strokes",
...         "password":"john123",
...         "email":"Strokes@gmail.com",
...         "user_type":"o"
...     })
WriteResult({ "nInserted" : 1 })
> db.users.insert({
...         "username":"Pitter",
...         "name":"Pitter Glitter",
...         "password":"jane123",
...         "email":"Pitter@gmail.com",
...         "user_type":"o"
