###### CREATE DOCUMENT ######
db.users.insert({
...         "username":"123nush",
...         "name":"Anushka",
...         "password":"anushka111",
...         "email":"anushka@gmail.com",
...         "user_type":"a"
...     })
WriteResult({ "nInserted" : 1 })


> show collections
users
> db.users.insert({
...         "username":"sanika18",
...         "name":"sanika",
...         "password":"sanika111",
...         "email":"sanika@gmail.com",
...         "user_type":"o"
...     })
WriteResult({ "nInserted" : 1 })
